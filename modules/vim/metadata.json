{
  "name": "dhoppe-vim",
  "version": "1.4.1",
  "author": "Dennis Hoppe",
  "summary": "This module installs and configures the Vim package.",
  "license": "Apache-2.0",
  "source": "git://github.com/dhoppe/puppet-vim.git",
  "project_page": "https://github.com/dhoppe/puppet-vim",
  "issues_url": "https://github.com/dhoppe/puppet-vim/issues",
  "dependencies": [
    {"name":"puppet/extlib","version_requirement":">= 1.0.0 < 2.0.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">= 4.5.0 < 5.0.0"}
  ],
  "data_provider": null,
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0"
    }
  ],
  "tags": [
    "vim"
  ]
}
